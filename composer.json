{
    "name": "koubas/mongofill",
    "description": "Polyfill of PECL Mongo extension",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Husák",
            "email": "husak.j@gmail.com"
        },
        {
            "name":  "Máximo Cuadros",
            "email": "mcuadros@gmail.com"
        },
        {
            "name": "Mongofill Community",
            "homepage": "https://github.com/koubas/mongofill/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "athletic/athletic": "dev-master"
    },
    "autoload": {
        "classmap": [
            { "Mongofill\\Util": "src/Mongofill/Util.php" },
            { "Mongofill\\Bson": "src/Mongofill/Bson.php" },
            { "Mongofill\\Protocol": "src/Mongofill/Protocol.php" },
            { "MongoBinData": "src/MongoBinData.php" },
            { "MongoClient": "src/MongoClient.php" },
            { "Mongo": "src/Mongo.php" },
            { "MongoDB": "src/MongoDB.php" },
            { "MongoDBRef": "src/MongoDBRef.php" },
            { "MongoDate": "src/MongoDate.php" },
            { "MongoCollection": "src/MongoCollection.php" },
            { "MongoCursor": "src/MongoCursor.php" },
            { "MongoId": "src/MongoId.php" },
            { "MongoRegex": "src/MongoRegex.php" },
            { "MongoInt32": "src/MongoInt32.php" },
            { "MongoInt64": "src/MongoInt64.php" },
            { "MongoCode": "src/MongoCode.php" },
            { "MongoException": "src/MongoException.php" },
            { "MongoLog": "src/MongoLog.php" },
            { "MongoTimestamp": "src/MongoTimestamp.php" },
            { "MongoGridFS": "src/MongoGridFS.php" },
            { "MongoGridFSFile": "src/MongoGridFSFile.php" },
            { "MongoGridFSException": "src/MongoGridFSException.php" },
            { "MongoGridFSCursor": "src/MongoGridFSCursor.php" }
        ],
        "files": [
            "src/functions.php"
        ]
    }
}
